{block foot}
{control js}

<script>
	$(document).ready(function () {	
		$("body").stream({
			rows: {$count}
		});
	});
	$("#frmaddItemForm-note").click(function() {
		$("#frmaddItemForm-note").animate({ height: "200px"}, 500);
	});
</script>

{/block}


