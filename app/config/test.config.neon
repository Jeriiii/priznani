common:

	includes:
		- model.neon
		- testconection.neon
	
	parameters:

	php:
		date.timezone: Europe/Prague
		# session.save_path: "%tempDir%/sessions"
		# zlib.output_compression: yes


	nette:
		session:
			autoStart: smart
			expiration: +5 days

		database:
			default:
				dsn: '%database.driver%:host=%database.host%;dbname=%database.dbname%'
				user: %database.user%
				password: %database.password%


	services:
		
		userManager: \Test\UserManager(@POS\Model\UserDao)

		authorizator:
			class: Acl

		
production < common:


development < common:
