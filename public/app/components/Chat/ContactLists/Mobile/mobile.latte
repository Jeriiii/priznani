<ul id="contacts" data-role="listview" data-filter="true" data-inset="true" data-input="#rich-autocomplete-input">
	{foreach $contacts as $contact}
		{var $lastActivity = $contactList->getLastActivity($contact->user2)}
		{var $id = $coder->encodeData($contact->user2->id)}
